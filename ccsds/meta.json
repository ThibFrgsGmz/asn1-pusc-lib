{
  "name": "CCSDS",
  "submodules": [
    {
      "name": "CCSDS Packets",
      "elements": [
        {
          "name": "Packet Types",
          "asn1Files": ["PacketTypes.asn1"],
          "imports": [
            {
              "from": "ApplicationProcess",
              "types": ["ApplicationProcess-ID"]
            }
          ]
        },
        {
          "name": "TC Packet",
          "asn1Files": ["TC-Packet.asn1"],
          "requires": ["Packet Types"],
          "imports": [
            {
              "from": "PacketTypes",
              "types": [
                "CCSDS-Packet",
                "PacketDataField",
                "Packet-ID",
                "MessageType-ID",
                "PacketPrimaryHeader",
                "UserDataField",
                "SecondaryHeaderFlag"
              ]
            },
            {
              "from": "ApplicationProcessUser",
              "types": ["ApplicationProcessUser-ID"]
            },
            {
              "from": "TC-Payload",
              "types": [
                "TC-Data",
                "TC-NoHeaderData"
              ]
            }
          ]
        },
        {
          "name": "TM Packet",
          "asn1Files": ["TM-Packet.asn1"],
          "requires": ["Packet Types"],
          "imports": [
            {
              "from": "PacketTypes",
              "types": [
                "CCSDS-Packet",
                "PacketDataField",
                "Packet-ID",
                "MessageType-ID",
                "PacketPrimaryHeader",
                "UserDataField",
                "SecondaryHeaderFlag"
              ]
            },
            {
              "from": "ApplicationProcessUser",
              "types": ["ApplicationProcessUser-ID"]
            },
            {
              "from": "BasicTypes",
              "types": ["AbsoluteTime"]
            },
            {
              "from": "TM-Payload",
              "types": [
                "TM-Data",
                "TM-NoHeaderData"
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "Mission Objects",
      "elements": [
        {
          "name": "Application Process User",
          "asn1Files": ["ApplicationProcessUser.asn1"]
        },
        {
          "name": "Packet Error Control",
          "asn1Files": ["PacketErrorControl.asn1"]
        },
        {
          "name": "TM Payload",
          "asn1Files": ["TM-Payload.asn1"],
          "imports": [
            {
              "from": "PacketTypes",
              "types": ["MessageType-ID"]
            }
          ]
        },
        {
          "name": "TC Payload",
          "asn1Files": ["TC-Payload.asn1"],
          "imports": [
            {
              "from": "PacketTypes",
              "types": ["MessageType-ID"]
            }
          ]
        }
      ]
    }
  ]
}
