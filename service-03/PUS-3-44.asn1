PUS-3-44 DEFINITIONS AUTOMATIC TAGS ::= BEGIN
EXPORTS ALL;
IMPORTS
    PUSC-UINT32 FROM BasicTypes
    ApplicationProcess-ID FROM ApplicationProcess
    ParameterReportStructureType,
    ParameterReportStructure-ID FROM ParameterReportStructureType
    PeriodicGenerationActionStatus FROM PeriodicGenerationActionStatus
    CollectionInterval FROM CollectionInterval
    ParameterFunctionalReportingDefinition-ID FROM ParameterFunctionalReportingDefinition;

macTC-44-ParameterReportingEntryCount INTEGER ::= 536870911
maxTC-44-ApidParameterReportingEntryCount INTEGER ::= 536870911

TC-3-44-ModifyThePeriodicGenerationPropertiesOfParameterReportDefinitionsOfAParameterFunctionalReportingDefinitionMultipleApid ::= TC-3-44-ModifyThePeriodicGenerationPropertiesOfParameterReportDefinitionsOfAParameterFunctionalReportingDefinitionMultipleApidGeneric {ParameterFunctionalReportingDefinition-ID}

TC-3-44-ModifyThePeriodicGenerationPropertiesOfParameterReportDefinitionsOfAParameterFunctionalReportingDefinitionMultipleApidGeneric {TParameterFunctionalReportingDefinition-ID} ::= SEQUENCE
{
    parameterFunctionalReportingDefinition-ID TParameterFunctionalReportingDefinition-ID,
--! n1 UnsignedInteger, moved to acn
    apidParameterReportingEntries SEQUENCE (SIZE(0 .. maxTC-44-ApidParameterReportingEntryCount)) OF ApidParameterReportingEntries
}

TC-3-44-ModifyThePeriodicGenerationPropertiesOfParameterReportDefinitionsOfAParameterFunctionalReportingDefinition ::= TC-3-44-ModifyThePeriodicGenerationPropertiesOfParameterReportDefinitionsOfAParameterFunctionalReportingDefinitionGeneric {ParameterFunctionalReportingDefinition-ID}

TC-3-44-ModifyThePeriodicGenerationPropertiesOfParameterReportDefinitionsOfAParameterFunctionalReportingDefinitionGeneric {TParameterFunctionalReportingDefinition-ID} ::= SEQUENCE
{
    parameterFunctionalReportingDefinition-ID TParameterFunctionalReportingDefinition-ID,
--! n2 UnsignedInteger, moved to acn
    parameterReportingEntries SEQUENCE (SIZE(0 .. macTC-44-ParameterReportingEntryCount)) OF ParameterReportingEntries
}

ApidParameterReportingEntries ::= ApidParameterReportingEntriesGeneric {ApplicationProcess-ID}

ApidParameterReportingEntriesGeneric {TApplicationProcess-ID} ::= SEQUENCE
{
    applicationProcess-ID ApplicationProcess-ID,
--! n2 UnsignedInteger, moved to acn
    parameterReportingEntries SEQUENCE (SIZE(0 .. macTC-44-ParameterReportingEntryCount)) OF ParameterReportingEntries
}

ParameterReportingEntries ::= ParameterReportingEntriesGeneric {ParameterReportStructureType, ParameterReportStructure-ID, PeriodicGenerationActionStatus, CollectionInterval}

ParameterReportingEntriesGeneric {TParameterReportStructureType, TParameterReportStructure-ID, TPeriodicGenerationActionStatus, TCollectionInterval} ::= SEQUENCE
{
--! parameterReportStructureType TParameterReportStructureType, moved to acn
    parameterReportStructure-ID TParameterReportStructure-ID,
    periodicGenerationActionStatus TPeriodicGenerationActionStatus,
    collectionInterval TCollectionInterval
}

END
