PUS-3-10 DEFINITIONS AUTOMATIC TAGS ::= BEGIN
EXPORTS ALL;
IMPORTS
    HousekeepingParameterReportStructure-ID FROM HousekeepingParameterReportStructure
    Parameter-ID FROM Parameter
    PUSC-UINT32 FROM BasicTypes
    PeriodicGenerationActionStatus FROM PeriodicGenerationActionStatus
    CollectionInterval FROM CollectionInterval;


maxTC-3-10-Parameter-ID-Count INTEGER ::= 1073741823
maxTC-3-10-SuperComutatedParameterSetCount INTEGER ::= 536870911

TC-3-10-HousekeepingParameterReportStructureReport ::= TC-3-10-HousekeepingParameterReportStructureReportGeneric {HousekeepingParameterReportStructure-ID, PeriodicGenerationActionStatus, CollectionInterval, Parameter-ID}

TC-3-10-HousekeepingParameterReportStructureReportGeneric {THousekeepingParameterReportStructure-ID, TPeriodicGenerationActionStatus, TCollectionInterval, TParameter-ID} ::= SEQUENCE
{
    housekeepingParameterReportStructure-ID THousekeepingParameterReportStructure-ID,
    periodicGenerationActionStatus TPeriodicGenerationActionStatus OPTIONAL, --TODO
    collectionInterval TCollectionInterval,
--! n1 UnsignedInteger, moved to acn
    parameter-ID SEQUENCE (SIZE(0 .. maxTC-3-10-Parameter-ID-Count)) OF TParameter-ID,
--! nfa UnsignedInteger, moved to acn
    superCommutatedParameterSet SEQUENCE (SIZE(0 .. maxTC-3-10-SuperComutatedParameterSetCount)) OF SuperCommutatedParameterSet
}

SuperCommutatedParameterSet ::= SuperCommutatedParameterSetGeneric {PUSC-UINT32, Parameter-ID}

SuperCommutatedParameterSetGeneric {UnsignedInteger, TParameter-ID} ::= SEQUENCE
{
    superCommutatedSampleRepetitionNumber UnsignedInteger,
--! n2 UnsignedInteger, moved to acn
    parameter-ID SEQUENCE (SIZE(0 .. maxTC-3-10-Parameter-ID-Count)) OF TParameter-ID
}

END
