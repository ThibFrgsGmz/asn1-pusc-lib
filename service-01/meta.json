{
  "name": "ST[01] request verification",
  "submodules": [
    {
      "name": "Routing and reporting subservice",
      "elements": [
        {
          "name": "Routing verification of a request"
        },
        {
          "name": "Reporting failed routing",
          "asn1Files": ["PUS-1-10.asn1"],
          "requires": [
            "Request",
            "ErrorCodes",
            "Routing verification of a request"
          ]
        }
      ]
    },
    {
      "name": "Acceptance and reporting subservice",
      "elements": [
        {
          "name": "Acceptance verification of a request"
        },
        {
          "name": "Reporting successful acceptance",
          "asn1Files": ["PUS-1-1.asn1"],
          "requires": [
            "Request",
            "Acceptance verification of a request"
          ]
        },
        {
          "name": "Reporting failed acceptance",
          "asn1Files": ["PUS-1-2.asn1"],
          "requires": [
            "Request",
            "Error Codes",
            "Acceptance verification of a request"
          ]
        }
      ]
    },
    {
      "name": "Execution reporting subservice",
      "elements": [
        {
          "name": "Reporting successful start of execution",
          "asn1Files": ["PUS-1-3.asn1"],
          "requires": ["Requires"]
        },
        {
          "name": "Reporting failed start of execution",
          "asn1Files": ["PUS-1-4.asn1"],
          "requires": [
            "Request",
            "Error Codes"
          ]
        },
        {
          "name": "Reporting successful progress of execution",
          "asn1Files": ["PUS-1-5.asn1"],
          "imports": [
            {
              "from": "ExecutionStep",
              "types": ["ExecutionStep-ID"]
            }
          ],
          "requires": ["Request"]
        },
        {
          "name": "Reporting failed progress of execution",
          "asn1Files": ["PUS-1-6.asn1"],
          "imports": [
            {
              "from": "ExecutionStep",
              "types": ["ExecutionStep-ID"]
            }
          ],
          "requires": [
            "Request",
            "Error Codes"
          ]
        },
        {
          "name": "Reporting successful completion of execution",
          "asn1Files": ["PUS-1-7.asn1"],
          "requires": ["Request"]
        },
        {
          "name": "Reporting failed completion of execution",
          "asn1Files": ["PUS-1-8.asn1"],
          "requires": [
            "Request",
            "Error Codes"
          ]
        }
      ]
    },
    {
      "name": "Components",
      "elements": [
        {
          "name": "Request",
          "asn1Files": ["Request.asn1"],
          "imports": [
            {
              "from": "PacketTypes",
              "types": [
                "PacketVersionNumber",
                "TC-Packet-ID",
                "PacketSequenceControl"
              ]
            }
          ]
        },
        {
          "name": "Error Codes",
          "asn1Files": ["ErrorCodes.asn1"]
        }
      ]
    }
  ]
}
